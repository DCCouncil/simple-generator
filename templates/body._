<h1><%= title %></h1>

<% body.forEach(function(group) { %>
    <div class="<%= group.group %>">
    <% group.paras.forEach(function(l) { %>
        <p class="<%= l.class %>" style="padding-left: <%= l.indentation*30 %>px">
            <% l.text.forEach(function(span) { %>
                <span class="<%- span.class %>" style="<%- span.style %>"><%= span.text %></span>
            <% }); %>
        </p>
    <% }); %>
    </div>
<% }); %>

<% if (children) { %>
    <ul id="page_children">
    <% children.forEach(function(s) { %>
        <li <% if (s.is_placeholder) { %>class="placeholder"<% } %>>
            <div class="title"><a href="<%= s.filename%>"><%= s.title%></a></div>
            <% if (s.section_range[0] && s.section_range[1]) { %>
                <div class="range">§§ <%- s.section_range[0] %> &ndash; <%- s.section_range[1] %></div>
            <% } %>
        </li>
    <% }); %>
    </ul>
<% } %>
