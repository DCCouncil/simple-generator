<!DOCTYPE html>
<html>
    <head>
        <title>ยง <%= level.type %> <%= level.num %>: <%= level.heading %></title>
        <meta http-equiv='content-type' content='text/html; charset=utf-8' />
        <link rel='stylesheet' media='screen' type='text/css' href='/css/base.css' />
    </head>
    <body>
        <header>
            <div class='right'>
                <a href='/'>home</a>,
                <a href='about.html'>about</a>
            </div>
            <h1>The DC Code: ยง <%= level.type %> <%= level.num %>: <%= level.heading %></h1>
            <a href='../'>Index</a>
        </header>
        <%= level.text %>
        <% (level.level || []).forEach(function(l) { %>
            <%= l.type %>
            <% if (l.level) { (Array.isArray(l.level) ? l.level : [l.level]).forEach(function(sublevel) { %>
                <h3><%= sublevel.heading %></h3>
                <div><%= sublevel.text %></div>
            <% }); } %>
        <% }); %>
    </body>
</html>
