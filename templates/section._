<!DOCTYPE html>
<html>
    <head>
        <title>
            DC Code <%= title %>
        </title>
        <meta http-equiv='content-type' content='text/html; charset=utf-8' />
        <link rel='stylesheet' media='screen' type='text/css' href='/css/base.css' />
    </head>
    <body>
        <header>
            <div class='right'>
                <a href='/'>home</a>,
                <a href='about.html'>about</a>
            </div>
            <h1>
                <%= title %>
            </h1>
            <% if (is_index_file) { %>
                <p><a href='../'>Up a Level</a></p>
            <% } else { %>
                <p><a href='index.html'>Up a Level</a></p>
            <% } %>
        </header>

        <% d.level.text.forEach(function(t) { %>
            <%= cited(t) %>
        <% }); %>
        <% (d.level.level || []).forEach(function(l) { %>
            <%= l.type %>
            <% if (l.level) { (Array.isArray(l.level) ? l.level : [l.level]).forEach(function(sublevel) { %>
                <h3><%= sublevel.heading %></h3>
                <% if (!Array.isArray(sublevel.text)) sublevel.text = [sublevel.text] %>
                <% if (sublevel.text) { sublevel.text.forEach(function(t) { %>
                    <div><%= cited(t) %></div>
                <% }); } %>
            <% }); } %>
        <% }); %>

        <% if (children) { %>
            <ul>
            <% children.forEach(function(s) { %>
                <li><a href="<%= s.filename%>"><%= s.title%></a></li>
            <% }); %>
            </ul>
        <% } %>
    </body>
</html>
